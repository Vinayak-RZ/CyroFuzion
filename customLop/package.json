{
  "name": "@1inch/limit-order-protocol-contract",
  "version": "4.3.2",
  "description": "1inch Limit Order Protocol",
  "repository": {
    "type": "git",
    "url": "git@github.com:1inch/limit-order-protocol.git"
  },
  "license": "MIT",
  "files": [
    "contracts",
    "test/helpers"
  ],
  "dependencies": {
    "@1inch/solidity-utils": "6.6.0",
    "@chainlink/contracts": "0.8.0",
    "@gnosis.pm/safe-contracts": "1.3.0",
    "@openzeppelin/contracts": "5.1.0"
  },
  "devDependencies": {
    "@1inch/solidity-utils": "^3.5.1",
    "@gnosis.pm/safe-contracts": "1.3.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.22.0",
    "hardhat-dependency-compiler": "^1.2.0",
    "hardhat-deploy": "^0.12.0",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-tracer": "^2.6.0",
    "solidity-coverage": "^0.8.0",
    "solidity-docgen": "0.5.16",
    "dotenv": "^16.0.0"
  },
  "scripts": {
    "clean": "rimraf artifacts cache coverage contracts/hardhat-dependency-compiler artifacts-zk cache-zk",
    "deploy": "hardhat deploy --network",
    "coverage": "hardhat coverage",
    "docify": "yarn hardhat docgen",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:sol:fix": "solhint --max-warnings 0 \"contracts/**/*.sol\" --fix",
    "lint": "yarn run lint:js && yarn run lint:sol",
    "lint:fix": "yarn run lint:js:fix && yarn run lint:sol:fix",
    "test": "hardhat test --parallel",
    "test:ci": "hardhat test"
  },
  "resolutions": {
    "elliptic": "6.6.1"
  }
}